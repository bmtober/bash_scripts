. assert

echo "Testing union"

../union data/union.dat > results/union.1
../union < data/union.dat > results/union.2
cat data/union.dat | ../union > results/union.3

echo
assert 'diff expected/union.out results/union.1 &> /dev/null'
assert 'diff expected/union.out results/union.2 &> /dev/null'
assert 'diff expected/union.out results/union.3 &> /dev/null'

echo
echo "PASS: ${success_count}"
echo "FAIL: ${failure_count}"
